{
  "name": "budget-badger",
  "version": "0.1.0",
  "private": true,
  "main": "dist/electron.js",
  "type": "module",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "electron-is-dev": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
